<h4 class="alert alert-warning">[% l('This section is for profiling. Fingerbank will help you every device type.') %]</h4>

<div id="tabView">
  <ul class="nav nav-tabs">
    <li [% IF tab == 'general settings' %]class="active"[% END %]>
      <a href="#newTabView" data-href="[% c.uri_for(c.controller('Config::Fingerbank::Settings').action_for('index')) %]" data-toggle="tab">[% l('General Settings') %]</a>
    </li>
    <li [% IF tab == 'combination' %]class="active"[% END %]>
      <a href="#newTabView" data-href="[% c.uri_for(c.controller('Config::Fingerbank::Combination').action_for('index')) %]" data-toggle="tab">[% l('Combination') %]</a>
    </li>
    <li [% IF tab == 'devices' %]class="active"[% END %]>
      <a href="#newTabView" data-href="[% c.uri_for(c.controller('Config::Fingerbank::Device').action_for('index')) %]" data-toggle="tab">[% l('Devices') %]</a>
    </li>
    <li [% IF tab == 'dhcp fingerprints' %]class="active"[% END %]>
      <a href="#newTabView" data-href="[% c.uri_for(c.controller('Config::Fingerbank::DHCP_Fingerprint').action_for('index')) %]" data-toggle="tab">[% l('DHCP Fingerprints') %]</a>
    </li>
    <li [% IF tab == 'dhcp vendors' %]class="active"[% END %]>
      <a href="#newTabView" data-href="[% c.uri_for(c.controller('Config::Fingerbank::DHCP_Vendor').action_for('index')) %]" data-toggle="tab">[% l('DHCP Vendors') %]</a>
    </li>
    <li [% IF tab == 'dhcpv6 fingerprints' %]class="active"[% END %]>
