    </li>
    <li [% IF tab == 'mac vendors' %]class="active"[% END %]>
      <a href="#newTabView" data-href="[% c.uri_for(c.controller('Config::Fingerbank::MAC_Vendor').action_for('index')) %]" data-toggle="tab">[% l('MAC Vendors') %]</a>
    </li>
    <li [% IF tab == 'user agents' %]class="active"[% END %]>
